[
	{
		"id": "chat_1",
		"messages": [
			{
				"id": "msg_1",
				"text": "Hello there!",
				"userId": "user_1"
			},
			{
				"id": "msg_2",
				"text": "General Kenobi!",
				"userId": "user_2"
			},
			{
				"id": "msg_3",
				"text": "I’ve heard...",
				"userId": "user_1"
			},
			{
				"id": "msg_4",
				"text": "you’ve been making some waves lately.",
				"userId": "user_1"
			},
			{
				"id": "msg_5",
				"text": "What can I say?",
				"userId": "user_2"
			},
			{
				"id": "msg_6",
				"text": "I’ve got the high ground now.",
				"userId": "user_2"
			},
			{
				"id": "msg_7",
				"text": "Any advice for me, Obi-Wan?",
				"userId": "user_1"
			},
			{
				"id": "msg_8",
				"text": "Always keep calm...",
				"userId": "user_2"
			},
			{
				"id": "msg_9",
				"text": "even in a duel with a Sith Lord.",
				"userId": "user_2"
			},
			{
				"id": "msg_10",
				"text": "Good advice.",
				"userId": "user_1"
			},
			{
				"id": "msg_11",
				"text": "I’ll take it to heart.",
				"userId": "user_1"
			},
			{
				"id": "msg_12",
				"text": "How’s Qui-Gon?",
				"userId": "user_2"
			},
			{
				"id": "msg_13",
				"text": "Still meditating, as usual.",
				"userId": "user_1"
			},
			{
				"id": "msg_14",
				"text": "He’s at peace.",
				"userId": "user_1"
			},
			{
				"id": "msg_15",
				"text": "Glad to hear that.",
				"userId": "user_2"
			},
			{
				"id": "msg_16",
				"text": "The galaxy is restless lately.",
				"userId": "user_2"
			},
			{
				"id": "msg_17",
				"text": "Agreed.",
				"userId": "user_1"
			},
			{
				"id": "msg_18",
				"text": "But we Jedi...",
				"userId": "user_1"
			},
			{
				"id": "msg_19",
				"text": "must maintain the balance.",
				"userId": "user_1"
			},
			{
				"id": "msg_20",
				"text": "Wise words.",
				"userId": "user_2"
			},
			{
				"id": "msg_21",
				"text": "What’s next on your mission?",
				"userId": "user_1"
			},
			{
				"id": "msg_22",
				"text": "Headed to Mustafar.",
				"userId": "user_2"
			},
			{
				"id": "msg_23",
				"text": "Something important awaits.",
				"userId": "user_2"
			},
			{
				"id": "msg_24",
				"text": "Mustafar...",
				"userId": "user_1"
			},
			{
				"id": "msg_25",
				"text": "That sounds ominous.",
				"userId": "user_1"
			},
			{
				"id": "msg_26",
				"text": "Let’s hope it’s not as bad as it seems.",
				"userId": "user_2"
			},
			{
				"id": "msg_27",
				"text": "Stay vigilant, Obi-Wan.",
				"userId": "user_1"
			},
			{
				"id": "msg_28",
				"text": "Always.",
				"userId": "user_2"
			},
			{
				"id": "msg_29",
				"text": "May the Force be with you.",
				"userId": "user_1"
			},
			{
				"id": "msg_30",
				"text": "And with you.",
				"userId": "user_2"
			},
			{
				"id": "msg_31",
				"text": "By the way...",
				"userId": "user_1"
			},
			{
				"id": "msg_32",
				"text": "Anakin mentioned you.",
				"userId": "user_1"
			},
			{
				"id": "msg_33",
				"text": "Did he?",
				"userId": "user_2"
			},
			{
				"id": "msg_34",
				"text": "What did he say?",
				"userId": "user_2"
			},
			{
				"id": "msg_35",
				"text": "That he respects you...",
				"userId": "user_1"
			},
			{
				"id": "msg_36",
				"text": "but finds you...",
				"userId": "user_1"
			},
			{
				"id": "msg_37",
				"text": "a bit rigid sometimes.",
				"userId": "user_1"
			},
			{
				"id": "msg_38",
				"text": "Haha, fair enough.",
				"userId": "user_2"
			},
			{
				"id": "msg_39",
				"text": "He’s young and ambitious.",
				"userId": "user_2"
			},
			{
				"id": "msg_40",
				"text": "He’ll grow into it.",
				"userId": "user_1"
			},
			{
				"id": "msg_41",
				"text": "Let’s hope so.",
				"userId": "user_2"
			},
			{
				"id": "msg_42",
				"text": "The future is uncertain...",
				"userId": "user_1"
			},
			{
				"id": "msg_43",
				"text": "but we have hope.",
				"userId": "user_1"
			},
			{
				"id": "msg_44",
				"text": "Yes, always hope.",
				"userId": "user_2"
			},
			{
				"id": "msg_45",
				"text": "Until we meet again, my friend.",
				"userId": "user_1"
			},
			{
				"id": "msg_46",
				"text": "Until then.",
				"userId": "user_2"
			},
			{
				"id": "msg_47",
				"text": "The Force will guide us both.",
				"userId": "user_1"
			},
			{
				"id": "msg_48",
				"text": "Yes, it will.",
				"userId": "user_2"
			},
			{
				"id": "msg_49",
				"text": "Let’s hope Anakin finds his way.",
				"userId": "user_1"
			},
			{
				"id": "msg_50",
				"text": "He will.",
				"userId": "user_2"
			},
			{
				"id": "msg_51",
				"text": "I believe in him.",
				"userId": "user_2"
			},
			{
				"id": "msg_52",
				"text": "As do I.",
				"userId": "user_1"
			},
			{
				"id": "msg_53",
				"text": "The path ahead is long...",
				"userId": "user_2"
			},
			{
				"id": "msg_54",
				"text": "but we will persevere.",
				"userId": "user_2"
			},
			{
				"id": "msg_55",
				"text": "Together.",
				"userId": "user_1"
			},
			{
				"id": "msg_56",
				"text": "Always.",
				"userId": "user_2"
			}
		],
		"users": {

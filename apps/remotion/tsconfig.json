{
  "extends": "eslint-config-react-native-wcandillon/tsconfig.base",
	"compilerOptions": {
		"target": "ES2018",
		"module": "commonjs",
		"jsx": "react-jsx",
		"outDir": "./dist",
		"strict": true,
		"noEmit": true,
		"lib": ["es2015"],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noUncheckedIndexedAccess": false
	}
}
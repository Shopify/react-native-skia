{
  "name": "@shopify/react-native-skia",
  "publishConfig": {
    "access": "public"
  },
  "title": "React Native Skia",
  "version": "0.1.100",
  "description": "High-performance React Native Graphics using Skia",
  "main": "index.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "README.md",
    "LICENSE.md",
    "android/build.gradle",
    "android/CMakeLists.txt",
    "android/cpp/**",
    "android/src/**",
    "libs/android/**",
    "src/**",
    "index.js",
    "cpp/**/*.{h,cpp}",
    "ios",
    "libs/ios/libskia.xcframework",
    "libs/ios/libskshaper.xcframework",
    "libs/ios/libsvg.xcframework",
    "react-native-skia.podspec",
    "scripts/install-npm.js",
    "dist/**"
  ],
  "scripts": {
    "tsc": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx  --max-warnings 0",
    "test": "jest",
    "postinstall": "yarn node scripts/install-npm.js",
    "build": "bob build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shopify/react-native-skia.git",
    "baseUrl": "https://github.com/Shopify/react-native-skia"
  },
  "keywords": [
    "react-native"
  ],
  "author": "Shopify Inc.",
  "contributors": [
    {
      "name": "Christian Falch",
      "email": "christian.falch@gmail.com"
    },
    {
      "name": "William Candillon",
      "email": "wcandillon@gmail.com"
    }
  ],
  "license": "MIT",
  "licenseFilename": "LICENSE.md",
  "readmeFilename": "README.md",
  "peerDependencies": {
    "react": ">=16.8.1",
    "react-native": ">=0.63.0-rc.0 <1.0.x",
    "react-native-reanimated": ">=2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/react-native": "^0.65.0",
    "@types/react-reconciler": "^0.26.4",
    "eslint": "7.32.0",
    "eslint-config-react-native-wcandillon": "^3.7.2",
    "jest": "^27.4.3",
    "react": "17.0.2",
    "react-native": "0.66.2",
    "react-native-builder-bob": "^0.18.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "react-reconciler": "^0.26.2"
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
